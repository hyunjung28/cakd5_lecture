        "### 모델 정의"
      ],
      "metadata": {
        "id": "u4G73uIokH22"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "#데이터 생성\n",
        "X=torch.randn(200,1)*10\n",
        "y = X + 3 * torch.randn(200,1)\n",
        "plt.scatter(X.numpy(),y.numpy())\n",
        "plt.ylabel('y')\n",
        "plt.xlabel('x')\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
